{
  "tasks": {
    "dart": "dart compile wasm -O4 -E --enable-experimental-wasm-interop -E --js-runtime-output=dart.wasm.mjs -o dart.wasm lib/dart.dart",
    "zig": "zig build-exe native/zig.zig -target wasm32-freestanding -fno-entry --export=mandelbrot -O ReleaseSafe",
    "build": "deno task zig; deno task dart"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  }
}
